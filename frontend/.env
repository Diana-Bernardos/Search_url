# Frontend/.env

# API URLs
REACT_APP_API_URL=http://localhost:3001/api
REACT_APP_BASE_URL=http://localhost:3000

# Configuración del entorno
REACT_APP_ENV=development

# Timeouts
REACT_APP_API_TIMEOUT=30000

# Configuración de autenticación
REACT_APP_JWT_LOCAL_STORAGE_KEY=scraper_token
REACT_APP_USER_LOCAL_STORAGE_KEY=scraper_user

# Configuración de la aplicación
REACT_APP_TITLE=Web Scraper
REACT_APP_DESCRIPTION=Aplicación de Web Scraping
REACT_APP_VERSION=1.0.0

# Límites y configuraciones
REACT_APP_MAX_RESULTS_PER_PAGE=10
REACT_APP_MAX_HISTORY_ITEMS=50

# Mensajes de error personalizados
REACT_APP_DEFAULT_ERROR_MESSAGE=Algo salió mal. Por favor, inténtalo de nuevo.
REACT_APP_NETWORK_ERROR_MESSAGE=Error de conexión. Verifica tu conexión a internet.
REACT_APP_AUTH_ERROR_MESSAGE=Error de autenticación. Por favor, inicia sesión de nuevo.

# Feature flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_HISTORY=true
REACT_APP_ENABLE_FILTERS=true